<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>V_Contact_email_address_for_INtouch_reg</fullName>
    <active>true</active>
    <description>Contact must have an email address in order to be registered for INtouch</description>
    <errorConditionFormula>AND( ISPICKVAL(Division__c , &quot;VPS&quot;),
ISBLANK(Email), 
NOT(ISBLANK(TEXT( INtouch__c ))),
$User.ProfileId &lt;&gt; &apos;00e70000000jMBG&apos;)</errorConditionFormula>
    <errorMessage>Email address is required for INtouch registration</errorMessage>
</ValidationRule>
